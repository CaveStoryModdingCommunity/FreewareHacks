<?xml version="1.0" encoding="UTF-8"?>
<hack name="Remove Menu Bar" author="periwinkle">
	<panel>
		<panel>
			<field type="info">
			Removes the menu bar (the one containing the Quit/Volume/Version buttons).
			</field>
		</panel>
		<panel>
			<field type="data" offset="0x412787">
			90 90 90 90 90 90 90
			</field>
		</panel>
		<panel col="1">
			<field type="data" offset="0x41281F">
			4E 02 90
			</field>
		</panel>
		<panel>
			<field type="data" offset="0x412868">
			56 01 90
			</field>
		</panel>
	</panel>
</hack>